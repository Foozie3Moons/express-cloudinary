extends ./layout

block content
  h1 File Upload
  form(enctype='multipart/form-data' action='/' method='post')
    input(type='file' name='file')
    input(class='btn btn-primary' type='submit' value='Submit')
  h2 Images
  div.images
    each image in images
      - var url = cloudinary.url(image, {width: 250, height: 250});
      img(src=url)
